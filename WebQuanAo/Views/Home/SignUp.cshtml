
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Register</title>
    <link rel="stylesheet" href="../Content/style-Register.css">
</head>
<body>
    <div class="center">
        <img src="../Content/Images/Icon/kisspng-logo-brand-retail-price-book-store-5b1ff6bb22f1a4.9079057215288214351431-removebg-preview.png" alt="">
        <form method="post" action="#">
            <div class="txt_field">
                <input type="text" name="username" id="" autocomplete="off" required>
                <label for="label-account">Username</label>
            </div>
            <div class="txt_field">
                <input type="password" name="password" id="" autocomplete="off" required>
                <label for="label-account">Password</label>
            </div>
            <div class="txt_field">
                <input type="password" name="verify-password" id="" autocomplete="off" required>
                <label for="label-account">Verify Password</label>
            </div>
            <div class="txt_field">
                <input type="email" name="email" id="" autocomplete="off" required>
                <label for="label-account">Email</label>
            </div>
            <div class="txt_field">
                <input type="text" name="phone" id="" autocomplete="off" required>
                <label for="label-account">Phone</label>
            </div>
            <input type="submit" name="" value="Sign Up">
            <div class="login_link">
                Already Member? <a href="@Url.Action("Login","Home")">Login</a>
            </div>
        </form>
    </div>
</body>
</html>